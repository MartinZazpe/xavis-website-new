<body>


  <div id="background-image">
  </div>




  <section id="main-section">
    <div id="lateral-navbar" style="display:none">
      <div class="Lateral-nav-container">
        <div class="hambmenu-container-navbar">
          <div id="hamburguer-menu-navbar">
            <span class="bar1-nav"></span>
            <span class="bar2-nav"></span>
            <span class="bar3-nav"></span>
