    <div id="right-container">
      <div id="desktop-landing-images">
        <img src='../images/background/bg-image.jpg' alt="">
        <img id="resImage1200" class="noDisplay" src='../images/background/2C.jpg' alt="">
        <img src='../images/background/1C.jpg' alt="">
        <img src='../images/background/3C.jpg' alt="">
