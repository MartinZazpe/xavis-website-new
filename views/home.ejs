      <footer>
        <div class="social-links text-center">
          <i class="fa-brands fa-xl fa-instagram"></i>
          <i class="fa-brands fa-xl fa-linkedin"></i>
          <i class="fa-brands fa-xl fa-twitter"></i>
          <i class="fa-solid fa-xl fa-envelope"></i>
        </div>
